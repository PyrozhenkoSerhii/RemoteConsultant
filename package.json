{
  "name": "remote-advisor",
  "version": "1.0.0",
  "description": "There is a platform to replace a full-time staff consulatant with a part-time online consultant",
  "main": "main.js",
  "scripts": {
    "client-dev": "better-npm-run client-dev",
    "client-build": "better-npm-run client-build",
    "client-test": "better-npm-run client-test",
    "server-dev": "better-npm-run server-dev",
    "server-prod": "better-npm-run server-prod-pm-2",
    "server-test": "better-npm-run server-test"
  },
  "engines": {
    "node": "v8.11.3"
  },
  "betterScripts": {
    "client-dev": {
      "command": "webpack-dev-server --mode development --devtool inline-source-map --hot"
    },
    "client-build": {
      "command": "webpack --mode production"
    },
    "client-test": {
      "command": "jest"
    },
    "server-dev": {
      "command": "nodemon --exec babel-node server/server.js",
      "env": {
        "NODE_ENV": "dev"
      }
    },
    "server-prod-pm2": {
      "command": "pm2 start src/server.js --interpreter ./node_modules/.bin/babel-node",
      "env": {
        "NODE_ENV": "prod"
      }
    },
    "server-test": {
      "command": "mocha --require @babel/register --require mocha-clean --timeout 5000 ./tests/server/*.test.js",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "author": "Serhii Pyrozhenko",
  "repository": {
    "type": "git",
    "url": "git://github.com/PyrozhenkoSerhii/RemoteConsultant.git"
  },
  "license": "unlicensed",
  "devDependencies": {
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.0",
    "eslint": "^5.13.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "image-webpack-loader": "^4.6.0",
    "mocha": "^5.2.0",
    "mocha-clean": "^1.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.1",
